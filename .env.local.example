# Local secrets for kalshi-sys (sample values only)
# Copy to .env.local and replace with real credentials. Never commit secrets.

# Kalshi live trading credentials
KALSHI_API_KEY_ID=demo-access-key-id
KALSHI_PRIVATE_KEY_PEM_PATH=/absolute/path/to/kalshi_private_key.pem
KALSHI_ENV=prod

# Polygon (Massive) indices websocket access
POLYGON_API_KEY=demo-polygon-key

# Optional monitoring hooks
KALSHI_MONITOR_SLACK_WEBHOOK=https://hooks.slack.com/services/XXXX/XXXX/XXXX
